{
    "Version":"2012-10-17",
    "Id":"sns_key_policy",
    "Statement": [
      {
        "Sid": "IAM User Permissions",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::169002939622:root"
        },
        "Action": "kms:*",
        "Resource": "*"
      },
      {
        "Sid": "Allow key administration",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::169002939622:role/Admin"
        },
        "Action": [
          "kms:Create*",
          "kms:Describe*",
          "kms:Enable*",
          "kms:List*",
          "kms:Put*",
          "kms:Update*",
          "kms:Revoke*",
          "kms:Disable*",
          "kms:Get*",
          "kms:Delete*",
          "kms:ScheduleKeyDeletion",
          "kms:CancelKeyDeletion"
        ],
        "Resource": "*"
      },
      {
        "Sid": "Allow use",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::169002939622:role/Developer"
        },
        "Action": [
          "kms:Sign",
          "kms:Verify",
          "kms:DescribeKey"
        ],
        "Resource": "*"
      }
    ]
  }
